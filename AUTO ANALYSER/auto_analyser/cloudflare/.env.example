# Cloudflare API credentials
# Replace these with your actual credentials
# Do not commit the actual .env file to version control

# Cloudflare API token with Stream permissions
# Create this in the Cloudflare dashboard: https://dash.cloudflare.com/profile/api-tokens
CLOUDFLARE_API_TOKEN=your_api_token_here

# Your Cloudflare account ID
# Find this in the Cloudflare dashboard URL: https://dash.cloudflare.com/ACCOUNT_ID
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Optional: Port for the upload server (default: 3000)
PORT=3000
